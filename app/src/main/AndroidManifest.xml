        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:name">
        <activity android:name=".livedata.LiveDataActivity"></activity>
        <activity android:name=".mall.item.detail.MallItemDetailActivity" />
        <activity android:name=".arch.ArchActivity" />
        <activity android:name=".dagger.DaggerActivity" />
        <activity android:name=".launch.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.MainActivity" />
        <activity android:name=".custom.CustomViewActivity" />
        <activity android:name=".mvi.ui.MVITestActivity" />
        <activity android:name=".homewiki.list.HomeWikiListActivity" />
        <activity android:name=".homewiki.detail.HomeWikiDetailActivity" />
        <activity android:name=".homewiki.post.HomeWikiPostActivity" />
        <activity android:name=".homewiki.post.HomeWikiTagListActivity" />
        <activity android:name=".lazyfragment.LazyFragmentActivity" />
        <activity android:name=".test.Test1Activity" />
        <activity android:name=".test.Test2Activity" />
    </application>

</manifest>